@model IEnumerable<MTACodersLicence.Models.ProfessorKey>

@{
    ViewData["Title"] = "Professor keys";
}

<h2>Professor keys</h2>

<p>
    <a asp-action="Create" class="btn btn-success" style="float: right; margin-top: -20px;">Create New Key</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Key)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumberOfAccountsAvailable)
        </th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Key)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberOfAccountsAvailable)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-warning">Edit</a>
                <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>
<div>
    <a asp-controller="AdminManageAccounts" asp-action="Index" class="btn btn-default">Back to Accounts</a>
</div>
